            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".GUI.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GUI.classes"></activity>
        <activity android:name=".GUI.Admin.AdminDashboard"></activity>
        <activity android:name=".GUI.Admin.Edit_class_activity"></activity>
        <activity android:name=".GUI.User.UserDashboard"></activity>
        <activity android:name=".GUI.Trainer.TrainerDashboard"></activity>
    </application>

</manifest>